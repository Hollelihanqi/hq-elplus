{
  "name": "yto",
  "version": "0.1.0",
  "type": "module",
  "workspaces": [
    "packages/yto-custom",
    "packages/yto-utils"
  ],
  "scripts": {
    "build": "pnpm run -C packages/yto-custom build",
    "build:watch": "pnpm run -C packages/yto-custom watch",
    "build:utils": "pnpm run -C packages/yto-utils build",
    "play:dev": "pnpm run -C play dev",
    "docs:dev": "pnpm run -C docs docs:dev",
    "docs:build": "pnpm run -C docs docs:build",
    "docs:preview": "pnpm run -C docs docs:preview",
    "dev:all": "pnpm run -C play dev && pnpm run -C docs docs:dev",
    "lint": "eslint packages/yto-custom packages/yto-utils --ext .vue,.js,.ts,.jsx,.tsx,.json --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "commit": "git add -A && git-cz && git push",
    "publisher": "pnpm publish packages/yto-custom",
    "release:ui": "pnpm run -C packages/yto-custom release",
    "release": "npm run release --workspaces && release-it",
    "release:log": "release-it"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@release-it/bumper": "^5.1.0",
    "@release-it/conventional-changelog": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "cz-git": "^1.7.1",
    "element-plus": "^2.4.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-define-config": "^1.24.1",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "prettier": "^2.8.8",
    "release-it": "16.0.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.69.3",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.11",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vite-plugin-windicss": "^1.9.1",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.19",
    "windicss": "^3.5.6"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/runtime-core": "^3.3.4",
    "@vueuse/core": "^10.5.0",
    "@yto/custom": "workspace:^",
    "@yto/utils": "workspace:^",
    "echarts": "^5.4.3",
    "eslint-plugin-markdown": "^3.0.1",
    "fs-extra": "^11.1.1",
    "immutable": "^4.3.4",
    "moment": "^2.29.4",
    "print-js": "^1.6.0",
    "remove": "^0.1.5",
    "rimraf": "^5.0.5",
    "simple-uploader.js": "^0.6.0",
    "spark-md5": "^3.0.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "release-it": {
    "git": {
      "commit": false,
      "tag": false,
      "push": false
    },
    "npm": {
      "publish": false
    },
    "hooks": {
      "after:bump": "npx auto-changelog -p && git add CHANGELOG.md"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  }
}
