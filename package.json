{
  "name": "yto",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "pnpm run -C packages/yto-custom build",
    "build:watch": "pnpm run -C packages/yto-custom watch",
    "build:utils": "pnpm run -C packages/yto-utils build",
    "play:dev": "pnpm run -C play dev",
    "docs:dev": "pnpm run -C docs docs:dev",
    "docs:build": "pnpm run -C docs docs:build",
    "docs:preview": "pnpm run -C docs docs:preview",
    "dev:all": "pnpm run -C play dev && pnpm run -C docs docs:dev",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "commit": "git add -A && git-cz && git push",
    "publisher": "pnpm run -C packages/yto-custom/dist pnpm publish"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "cz-git": "^1.6.1",
    "element-plus": "^2.3.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-define-config": "^1.20.0",
    "eslint-plugin-vue": "^9.11.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "release-it": "^15.11.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.56.1",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-element-plus": "^0.4.1",
    "unplugin-vue-components": "^0.22.8",
    "vite": "^4.3.5",
    "vite-plugin-dts": "^2.2.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vite-plugin-windicss": "^1.8.10",
    "vue-eslint-parser": "^9.1.1",
    "vue-tsc": "^1.4.4",
    "windicss": "^3.5.6"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/runtime-core": "^3.2.47",
    "@yto/custom": "workspace:^",
    "@yto/utils": "workspace:^",
    "echarts": "^5.4.2",
    "eslint-plugin-markdown": "^3.0.0",
    "fs-extra": "^11.1.1",
    "immutable": "^4.3.0",
    "moment": "^2.29.4",
    "print-js": "^1.6.0",
    "remove": "^0.1.5",
    "rimraf": "^5.0.0",
    "simple-uploader.js": "^0.6.0",
    "spark-md5": "^3.0.2",
    "vue": "^3.2.45",
    "vue-router": "4"
  }
}
